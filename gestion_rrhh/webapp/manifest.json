{
  "_version": "1.32.0",
  "sap.app": {
    "id": "logaligroup.gestionrrhh",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "ZEMPLOYEES_SRV": {
        "uri": "/sap/opu/odata/sap/ZEMPLOYEES_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.97.2",
      "libs": {
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "logaligroup.gestionrrhh.i18n.i18n"
        }
      },
      "employeeModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "ZEMPLOYEES_SRV",
        "preload": true,
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Request",
          "refreshAfterChange": false,
          "useBatch": false,
          "defaultUpdateMethod": "Put"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "logaligroup.gestionrrhh.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMenu",
          "pattern": "",
          "target": [
            "TargetMenu"
          ]
        },
        {
          "name": "RouteCreateEmployee",
          "pattern": "CreateEmployee",
          "target": [
            "TargetCreateEmployee"
          ]
        },
        {
          "name": "RouteShowEmployee",
          "pattern": "ShowEmployee",
          "target": [
            "TargetShowEmployee"
          ]
        }
      ],
      "targets": {
        "TargetMenu": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Menu",
          "viewName": "Menu"
        },
        "TargetCreateEmployee": {
          "viewType": "XML",
          "viewName": "CreateEmployee",
          "viewId": "CreateEmployee",
          "transition": "slide",
          "clearControlAggregation": true
        },
        "TargetShowEmployee": {
          "viewType": "XML",
          "viewName": "ShowEmployee",
          "viewId": "ShowEmployee",
          "transition": "slide",
          "clearControlAggregation": true
        }
      }
    },
    "rootView": {
      "viewName": "logaligroup.gestionrrhh.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}